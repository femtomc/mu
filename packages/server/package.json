{
	"name": "@femtomc/mu-server",
	"version": "26.2.50",
	"description": "HTTP API server for mu status, work items, messaging setup, and web UI.",
	"keywords": [
		"mu",
		"server",
		"api",
		"web",
		"automation"
	],
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"mu-server": "./dist/cli.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist/**",
		"public/**"
	],
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"test": "bun test",
		"start": "bun run dist/cli.js"
	},
	"dependencies": {
		"@femtomc/mu-agent": "26.2.50",
		"@femtomc/mu-control-plane": "26.2.50",
		"@femtomc/mu-core": "26.2.50",
		"@femtomc/mu-forum": "26.2.50",
		"@femtomc/mu-issue": "26.2.50"
	}
}
